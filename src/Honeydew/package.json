{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run postcss && npm run sass",
    "build:prod": "npm run postcss && npm run sass --style=compressed",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:src": "npm run postcss --watch",
    "watch:transform": "npm run sass --watch",
    "postcss": "postcss src/scss/tailwind.scss -o src/scss/tailwind.transform.scss --verbose",
    "sass": "node node_modules/dart-sass/sass.js src/scss/tailwind.transform.scss wwwroot/css/site.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.8.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "dart-sass": "^1.25.0",
    "postcss": "^7.0.30",
    "postcss-cli": "^7.1.1",
    "postcss-scss": "^2.1.1",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "monaco-editor": "^0.20.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch"
    ]
  }
}
